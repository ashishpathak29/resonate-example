<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="xslt/class.xsl"?><class name="EdgeAnimation" namespace="com.crackerbarrel.components" extends="com.crackerbarrel.components.UIComponent" author="Christopher Swenson" version="0.0.1">	<description><![CDATA[Loads an Adobe Edge animation]]></description>	<constructor>	<param name="id" type="String">		<![CDATA[id of the object]]>	</param>	<param name="left" type="Int">		<![CDATA[left position of the object]]>	</param>	<param name="top" type="Int">		<![CDATA[top position of the object]]>	</param>	<param name="uiStyle" type="UIStyle=null">		<![CDATA[style data to pass to the object]]>	</param>	<param name="file" type="String">		<![CDATA[path to the Edge animation file]]>	</param>	<return type="Object"><![CDATA[the new EdgeAnimation]]>	</return></constructor>	<property name="file" type="String">		<![CDATA[The Edge file loaded into the object]]>	</property>	<method name="setSize">	<description><![CDATA[sets the size of the animation]]></description>	<param name="width" type="Int">		<![CDATA[the width of the object]]>	</param>	<param name="height" type="Int">		<![CDATA[the height of the object]]>	</param>	<return type="Void">	</return></method></class>