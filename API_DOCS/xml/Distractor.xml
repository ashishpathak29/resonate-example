<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="xslt/class.xsl"?><class name="Distractor" namespace="com.crackerbarrel.components" extends="com.crackerbarrel.components.CheckBox" author="Christopher Swenson" version="0.0.1">	<description><![CDATA[Sub-class of CheckBox that creates a clickable distractor]]></description>	<constructor>	<param name="id" type="String">		<![CDATA[id of the object]]>	</param>	<param name="left" type="Int">		<![CDATA[left position of the object]]>	</param>	<param name="top" type="Int">		<![CDATA[top position of the object]]>	</param>	<param name="uiStyle" type="CheckBoxStyle=null">		<![CDATA[style data to pass to the object]]>	</param>	<return type="Object"><![CDATA[the new Distractor]]>	</return></constructor>	<property name="correct" type="Boolean">		<![CDATA[Is this distractor the correct one?]]>	</property></class>