<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="xslt/class.xsl"?><class name="SWF" namespace="com.crackerbarrel.components" extends="com.crackerbarrel.components.UIComponent" author="Christopher Swenson" version="0.0.1">	<description><![CDATA[Wrapper class around a Flash Object]]></description>	<constructor>	<param name="id" type="String">		<![CDATA[The id of the SWF]]>	</param>	<param name="left" type="Int">		<![CDATA[left position of the SWF]]>	</param>	<param name="top" type="Int">		<![CDATA[top position of the SWF]]>	</param>	<param name="uiStyle" type="SVGStyle=null">		<![CDATA[style data to pass to the SWF]]>	</param>	<param name="file" type="String">		<![CDATA[the flash file to load]]>	</param>	<return type="SWF"><![CDATA[The new SWF Object]]>	</return></constructor>	<property name="file" type="String">		<![CDATA[The swf file path]]>	</property></class>