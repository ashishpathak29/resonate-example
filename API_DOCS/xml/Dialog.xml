<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="xslt/class.xsl"?><class name="Dialog" namespace="com.crackerbarrel.components" extends="com.crackerbarrel.components.UIComponent" author="Christopher Swenson" version="0.0.1">	<description><![CDATA[Wrapper class around the jQuery UI Dialog widget]]></description>	<constructor>	<param name="id" type="String">		<![CDATA[id of the object]]>	</param>	<param name="left" type="Int">		<![CDATA[left position of the object]]>	</param>	<param name="top" type="Int">		<![CDATA[top position of the object]]>	</param>	<param name="divID" type="String">		<![CDATA[id of the div to use as the dialog]]>	</param>	<param name="uiStyle" type="DialogStyle=null">		<![CDATA[style data to pass to the object]]>	</param>	<return type="Object"><![CDATA[the new CheckBox]]>	</return></constructor>	<property name="divID" type="String">		<![CDATA[The id of the div holding the dialog]]>	</property>	<method name="open">	<description><![CDATA[Opens the dialog]]></description>	<return type="Void">	</return></method>	<method name="close">	<description><![CDATA[Closes the dialog]]></description>	<return type="Void">	</return></method></class>